{
  "port": 6980,
  "musicPath": "~/Music",
  "dbPath": "~/digdat.json",
  "exPath": "~/diggerexport",
  "cachePath": "~/diggercache",
  "spawn": {
    "comment": "Optional platform command to run after server starts",
    "darwin": {
      "comment": "MacOS supports opening a URL with the default browser",
      "command": "open",
      "args": ["http://localhost:6980"],
      "options": {"stdio": "ignore"} },
    "win32": {
      "comment": "Theoretically, this should work.",
      "command": "start",
      "args": ["http://localhost:6980"],
      "options": {"shell": "true"} } }
}
